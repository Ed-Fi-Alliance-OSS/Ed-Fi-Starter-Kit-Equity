<?xml version="1.0" encoding="utf-8"?>
<!--
  This should be a properly formatted Web.config file with <configuration> and <connectionStrings> elements.
  -->
<configuration>
  <connectionStrings>
    <add name="EdFi_Ods" connectionString="server=.\SQLEXPRESS;database=EdFi_Ods;Integrated Security=true;" providerName="System.Data.SqlClient" />
    <add name="EdFi_Admin" connectionString="server=.\SQLEXPRESS;database=EdFi_Admin;Integrated Security=true;" providerName="System.Data.SqlClient" />
    <add name="EdFi_Security" connectionString="server=.\SQLEXPRESS;database=EdFi_Security;Integrated Security=true;" providerName="System.Data.SqlClient" />
    <add name="EdFi_master" connectionString="Database=master; Data Source=.\SQLEXPRESS; Trusted_Connection=True;" providerName="System.Data.SqlClient" />
    <add name="EduIdContext" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=EduID_Db;Trusted_Connection=True;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
    <add name="UniqueIdIntegrationContext" connectionString="Data Source=.\SQLEXPRESS;Database=UniqueId;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="databaseTemplate:populatedTemplateScript" value="GrandBend" />
